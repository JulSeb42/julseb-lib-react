{
	"name": "@julseb-lib/react",
	"version": "1.1.3",
	"type": "module",
	"private": false,
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup && cp ./src/lib/index.css ./dist/index.css",
		"dev": "vite --open",
		"prepublishOnly": "pnpm build",
		"plop": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=./plop/plopfile.ts",
		"plop:c": "pnpm plop component",
		"plop:p": "pnpm plop preview",
		"plop:st": "pnpm plop subtype",
		"cssgen": "cssjson src/lib/index.css --output src/data/styles.json"
	},
	"dependencies": {
		"@julseb-lib/utils": "^1.0.2",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/vite": "^4.1.17",
		"clsx": "^2.1.1",
		"csstype": "^3.2.3",
		"fuse.js": "^7.1.0",
		"postcss": "^8.5.6",
		"prism-react-renderer": "^2.4.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-icons": "^5.5.0",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^7.9.6",
		"react-toastify": "^11.0.5",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.17",
		"tailwindest": "^3.2.2"
	},
	"peerDependencies": {
		"react": ">=18.0.0 <20.0.0 || ^19.0.0",
		"react-dom": ">=18.0.0 <20.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@react-docgen/cli": "^3.0.4",
		"@types/figlet": "^1.7.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/react-router-dom": "^5.3.3",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"@vitejs/plugin-react": "^5.1.1",
		"autoprefixer": "^10.4.22",
		"axios": "^1.13.2",
		"chalk": "^5.6.2",
		"cross-env": "^10.1.0",
		"cssjson-cli": "^1.0.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"figlet": "^1.9.4",
		"globals": "^16.5.0",
		"plop": "^4.0.4",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"react-router-dom": "^7.9.6",
		"tailwindcss": "^4.1.17",
		"tsup": "^8.5.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.4",
		"vite-plugin-checker": "^0.11.0",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-eslint": "^1.8.1"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./types": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/types/index.js",
			"require": "./dist/types/index.cjs",
			"default": "./dist/types/index.js"
		},
		"./component-props": {
			"types": "./dist/types/components-props.d.ts",
			"import": "./dist/types/components-props.js",
			"require": "./dist/types/components-props.cjs",
			"default": "./dist/types/components-props.js"
		},
		"./index.css": {
			"require": "./dist/index.css",
			"import": "./dist/index.css",
			"default": "./dist/index.css"
		}
	},
	"types": "./dist/index.d.ts",
	"packageManager": "pnpm@10.23.0"
}
